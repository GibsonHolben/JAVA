
public class AddressDriver
{

	public static void main(String[] args) 
	{
		System.out.println("**********************************A1");
		//Testing the dafualt constructor
		Address A1 = new Address();
		System.out.println("************************************\n");
		
		
		System.out.println("**********************************A2");
